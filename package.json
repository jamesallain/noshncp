{
  "name": "vip",
  "version": "1.0.0",
  "description": "jobsite",
  "main": "index.js",
  "config": {
    "GMAIL_USER": "vip.nodemailer@gmail.com",
    "GMAIL_PASS": "dexter123"
  },
  "scripts": {
    "dev:server": "env NODE_ENV=development MONGO_URL=mongodb://localhost:27017/ ./node_modules/nodemon/bin/nodemon.js -L --exec ./node_modules/babel-cli/bin/babel-node.js -w js/server -e js js/server",
    "dev:client": "./node_modules/babel-cli/bin/babel-node.js ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.config.dev.js",
    "build:server": "env NODE_ENV=development ./node_modules/babel-cli/bin/babel.js js/server --out-dir dist/server --source-maps",
    "build:client": "./node_modules/babel-cli/bin/babel-node.js ./node_modules/webpack/bin/webpack.js --config webpack.config.prod.js",
    "prebuild": "rm -Rrf ./dist",
    "build": "npm run build:server",
    "postbuild": "npm run build:client",
    "prod:server": "env NODE_ENV=production MONGO_URL=mongodb://localhost:27018/ ./node_modules/forever/bin/forever -c node dist/server",
    "start": "npm run prod:server",
    "docker:build": "docker build -t pyratin/vip -f .dockerfile.app .",
    "docker:bash": "docker run --rm -it --entrypoint /app/.entrypoint.bash.sh -v $(pwd):/app -v $(pwd)/data/db:/data/db --name pyratin-vip pyratin/vip",
    "docker:run": "docker run --rm -it --entrypoint /app/.entrypoint.run.sh --name pyratin-vip pyratin/vip",
    "dev:data": "env NODE_ENV=development MONGO_URL=mongodb://localhost:27017/ ./node_modules/nodemon/bin/nodemon.js -L --exec ./node_modules/babel-cli/bin/babel-node.js -w js/data -e js js/data",
    "dev:profile:process": "env NODE_ENV=development MONGO_URL=mongodb://localhost:27017/ ./node_modules/nodemon/bin/nodemon.js -L --exec ./node_modules/babel-cli/bin/babel-node.js -w js/data/profileProcess.js -e js js/data/profileProcess.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/pyratin/vip.git"
  },
  "keywords": [
    "jobsite"
  ],
  "author": "pyratin@gmail.com",
  "license": "ISC",
